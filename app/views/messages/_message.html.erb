<div class="cont-<%= message.user.id %>">
  <div class="message-box msg-<%= message.user.id %> " >
    <% unless message.room.is_private %>
      <h6 class="name"> <%= message.user.name %> </h6>
    <% end %>
  <%= message.content %>
  <%= image_tag message.picture.url if message.picture? %>
  </div>
</div>